								</row>
								<row>
									<cell>1d8</cell>
									<cell>1d8</cell>
									<cell>1d10</cell>
								</row>
								<row>
									<cell>1d10</cell>
									<cell>1d10</cell>
									<cell>1d12</cell>
								</row>
								<row>
									<cell>1d12</cell>
									<cell>1d12</cell>
									<cell>1d12+1d4</cell>
								</row>
